<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-header">
            <a class="close_modal" style="border: none;" onclick="resetForm()" data-dismiss="modal">
                <span aria-hidden="true">
                    <img src="~/Image/client/modal_close.png" class="img-responsive zoom" />
                </span>
                <span class="sr-only">Close</span>
            </a>
        </div>
        <!-- content goes here -->
        <div class="modal-body">
            <div class="col-md-12 text-center">&nbsp;</div>
            <h1 class="text_title_modal">THÔNG BÁO</h1>
            <div class="col-md-12">
                <div class="col-md-12"><h2 id="txtmess" class="text_modal"></h2></div>
            </div>
        </div>
    </div>
</div>

@section scripts
{
    <script type="text/javascript">
        /*Menu*/
        function myFunction() {
            var x = document.getElementById("myLinks");
            if (x.style.display === "block") {
                x.style.display = "none";
            } else {
                x.style.display = "block";
            }
        }

        function changeBg(a_tag) {
            $(".menu_mobile").addClass("topmenu_a_bg");
        }

        /*News*/
        $('.carousel[data-type="multi"] .item').each(function () {
            var next = $(this).next();
            if (!next.length) {
                next = $(this).siblings(':first');
            }
            next.children(':first-child').clone().appendTo($(this));

            for (var i = 0; i < 2; i++) {
                next = next.next();
                if (!next.length) {
                    next = $(this).siblings(':first');
                }

                next.children(':first-child').clone().appendTo($(this));
            }
        });

        $('.carousel1[data-type="multi"] .item').each(function () {
            var next = $(this).next();
            if (!next.length) {
                next = $(this).siblings(':first');
            }
            next.children(':first-child').clone().appendTo($(this));

            for (var i = 0; i < 2; i++) {
                next = next.next();
                if (!next.length) {
                    next = $(this).siblings(':first');
                }

                next.children(':first-child').clone().appendTo($(this));
            }
        });


        $(window).scroll(function () {
            var y = $(this).scrollTop();
            if (y > 1400) {
                $('.thanhphan_img').show(1000);
            } else {
                $('.thanhphan_img').hide(1000);
            }
        });

        function formatNumber(num) {
            return num.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1.")
        };

        $('.counter-count').each(function () {
            $(this).prop('Counter', 0).animate({
                Counter: $(this).text()
            }, {
                duration: 5000,
                easing: 'swing',
                step: function (now) {
                    $(this).text(formatNumber(Math.ceil(now)));
                }
            });
        });

        function checkPhone(phone) {
            var phone_string = phone.value;
            var id = phone.id;
            if (phone_string.length == 0) {
                $('#' + id + '_normal').hide();
                $('#' + id + '_error').show();
                $('#' + id + '_error').html("Vui lòng nhập số điện thoại!");
            } else {
                if (phone_string.length < 10 || phone_string.length > 11) {
                    $('#' + id + '_normal').hide();
                    $('#' + id + '_error').show();
                    $('#' + id + '_error').html("Vui lòng nhập đúng định dạng số điện thoại");
                } else {
                    if (phone_string.startsWith("0") && phone_string.length > 10) {
                        $('#' + id + '_normal').hide();
                        $('#' + id + '_error').show();
                        $('#' + id + '_error').html("Vui lòng nhập đúng định dạng số điện thoại");
                    } else {
                        if (id == "phone1") {
                            //getInfoByPhone(phone_string);
                        }

                        $('#' + id + '_error').hide();
                        $('#' + id + '_error').html("");
                        $('#' + id + '_error').hide();
                    }
                }
            }
        }

        function checkPhoneTC(phone) {
            var phone_string = phone.value;
            var id = phone.id;
            if (phone_string.length == 0) {
                $('#' + id + '_normal').hide();
                $('#' + id + '_error').show();
                $('#' + id + '_error').html("Vui lòng nhập số điện thoại!");
            } else {
                if (phone_string.length < 10 || phone_string.length > 11) {
                    $('#' + id + '_normal').hide();
                    $('#' + id + '_error').show();
                    $('#' + id + '_error').html("Vui lòng nhập đúng định dạng số điện thoại");
                } else {
                    if (phone_string.startsWith("0") && phone_string.length > 10) {
                        $('#' + id + '_normal').hide();
                        $('#' + id + '_error').show();
                        $('#' + id + '_error').html("Vui lòng nhập đúng định dạng số điện thoại");
                    } else {
                        if (id == "phone1") {
                            //getInfoByPhone(phone_string);
                        }

                        $('#' + id + '_error').hide();
                        $('#' + id + '_error').html("");
                        $('#' + id + '_error').hide();
                    }
                }
            }
        }

        function validateCode(code) {
            var code_string = code.value;
            if (code_string.length == 0) {
                $('#code_error').show();
                $('#code_error').html("Vui lòng nhập mã cào!");
            } else if (code_string.length != 9) {
                $('#code_error').show();
                $('#code_error').html("Mã cào không đúng. Vui lòng nhập lại!");
            } else if (code_string.length == 9) {
                $('#code_error').hide();
            }
        }

        function checkQuantity(quantity) {
            let value = quantity.value;
            if (isNaN(value)) {
                $('#package_quantity').val('1');
            } else if (value < 0) {
                $('#package_quantity').val('1');
            }
        }

        var tichdiem = {
            init: function () {
                tichdiem.registerEvent();
            },
            registerEvent: function () {
                $('#submit').off('click').on('click', function () {
                    var phone1 = $('#phone1').val();
                    var code = $('#code').val();

                    $('#submit').attr('disabled', 'disabled');

                    //$('#loader').css('display', 'inline-block');
                    if (phone1.length == 0 && code.length != 0) {
                        $('#phone1_normal').hide();
                        $('#phone1_error').show();
                        $('#phone1_error').html("Vui lòng nhập số điện thoại!");

                        $('#submit').removeAttr("disabled");

                    } else if (phone1.length != 0 && code.length == 0) {
                        $('#code_normal').hide();
                        $('#code_error').show();
                        $('#code_error').html("Vui lòng nhập mã cào!");

                        $('#submit').removeAttr("disabled");

                    } else if (phone1.length == 0 && code.length == 0) {
                        $('#phone1_normal').hide();
                        $('#phone1_error').show();
                        $('#phone1_error').html("Vui lòng nhập số điện thoại!");
                        $('#code_normal').hide();
                        $('#code_error').show();
                        $('#code_error').html("Vui lòng nhập mã cào!");

                        $('#submit').removeAttr("disabled");

                    } else {
                        if (phone1.length < 10 || phone1.length > 11) {
                            $('#phone1_normal').hide();
                            $('#phone1_error').show();
                            $('#phone1_error').html("Vui lòng nhập đúng định dạng số điện thoại");

                            $('#submit').removeAttr("disabled");

                        } else {
                            $.ajax({
                                url: '/Home/CallKichHoat',
                                type: 'POST',
                                dataType: 'json',
                                data: {
                                    phone1: phone1,
                                    code: code,
                                },
                                success: function (response) {
                                    let message = response.message;
                                    message = message.replace("19006436", "<a style='color: red;font-weight: bold;' href='tel:19006436'>1900.6436</a>");
                                    message = message.replace("1900.6436", "<a style='color: red;font-weight: bold;' href='tel:19006436'>1900.6436</a>");
                                    message = message.replace("http://dehp.vn", "https://dehp.vn");
                                    message = message.replace("https://dehp.vn", "<a href='https://dehp.vn' target='_blank' style='color: blue;font-weight: bold;'>https://dehp.vn</a>");
                                    //console.log(response);
                                    if (response.success) {
                                        if (response.message == "3") {
                                            $('#exampleModal').modal('show');
                                            $('#txtmess').html("<span style='line-height: 2'>" + message + '</span>');
                                        } else if (response.type == "code") {
                                            $('#code_error').show();
                                            $('#code_normal').hide();
                                            $('#code_error').html(response.message);
                                        } else {
                                            //alert('e');
                                            $('#exampleModal').modal('show');
                                            $('#txtmess').html("<span style='line-height: 2'>" + message + '</span>');

                                            $('#code_error').hide();
                                            $('#phone1_error').hide();
                                        }
                                        //$('#loader').css('display', 'none');
                                    }
                                    else {//success false
                                        $('#exampleModal').modal('show');
                                        $('#txtmess').html("<span style='line-height: 2'>" + message + '</span>');
                                        $('#code_error').hide();
                                        $('#phone1_error').hide();
                                    }

                                    $('#submit').removeAttr("disabled");
                                },
                            });
                        }
                    }
                });
            },
            resetForm: function () {
                //$('#form')[0].reset();
            }
        }
        tichdiem.init();

        var tracuu = {
            init: function () {
                tracuu.registerEvent();
            },
            registerEvent: function () {
                $('#submit_tc').off('click').on('click', function () {
                    $('#submit_tc').attr('disabled', 'disabled');

                    var phone1 = $('#phone_tc').val();
                    var product = "";
                    if (phone1.length == 0) {
                        $('#phone_tc_normal').hide();
                        $('#phone_tc_error').show();
                        $('#phone_tc_error').html("Vui lòng nhập số điện thoại!");

                        $('#submit_tc').removeAttr("disabled");

                    } else {
                        //$('#loader').css('display', 'inline-block');
                        $.ajax({
                            url: '/Home/CallTracuu',
                            type: 'POST',
                            dataType: 'json',
                            data: {
                                phone1: phone1,
                                product: product
                            },
                            success: function (response) {
                                if (response.success) {
                                    let message = response.message;

                                    $('#exampleModal').modal('show');
                                    $('#txtmess').html("<span style='line-height: 2'>" + message + '</span>');
                                    $('#quantity').val('');
                                    $('#phone_tc_error').hide();
                                    $('#product_error').hide();
                                    //$('#loader').css('display', 'none');
                                    $('#phone_tc_error').hide();
                                    $('#product_error').hide();
                                }
                                else {//success false
                                    $('#exampleModal').modal('show');
                                    $('#txtmess').html("<span style='line-height: 2'>" + message + '</span>');
                                    $('#quantity').val('');
                                    $('#phone_tc_error').hide();
                                    $('#product_error').hide();
                                }

                                $('#submit_tc').removeAttr("disabled");
                            },
                        });
                    }
                });
            },
            resetForm: function () {
                //$('#form')[0].reset();
            }
        }
        tracuu.init();

        function resetForm() {
            //$('#form')[0].reset();
            $('#code').val("");
            $('#code_normal').hide();
            $('#code_error').hide();
        }
        $('#quantity').val('');
    </script>
}